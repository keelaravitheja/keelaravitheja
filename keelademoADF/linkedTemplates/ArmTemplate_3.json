								"type": "DatasetReference"
							},
							"name": "PayrollEmpdata"
						}
					],
					"transformations": [
						{
							"name": "filterpayrolldept"
						}
					],
					"scriptLines": [
						"source(output(",
						"          EmployeID as string,",
						"          Name as string,",
						"          Country as string,",
						"          Department as string",
						"     ),",
